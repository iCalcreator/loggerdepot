    "license": "LGPL-3.0-or-later",
    "keywords": [
        "log",
        "logger",
        "depot"
    ],
    "homepage": "https://kigkonsult.se",
    "authors": [
        {
            "name": "Kjell-Inge Gustafsson",
            "email": "ical@kigkonsult.se"
        }
